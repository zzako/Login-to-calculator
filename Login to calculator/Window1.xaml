<Window x:Class="Login_to_calculator.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login_to_calculator"
        mc:Ignorable="d"
        Title="Tempreture Convertor" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="pepe.png"/>
        </Grid.Background>
        <Label Content="Please Enter a Number you wish to convert:" HorizontalAlignment="Left" Margin="97,45,0,0" VerticalAlignment="Top" Width="471" FontSize="20" FontFamily="MV Boli" FontWeight="Bold" FontStyle="Italic" Background="#FFCF6363"/>
        <TextBox x:Name="TextBox1" HorizontalAlignment="Left" Height="50" Margin="97,131,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="222"/>
        <Button Content="To Fahrenheit" HorizontalAlignment="Left" Margin="499,131,0,0" VerticalAlignment="Top" Width="107" Height="50" Click="Button_Click_1"/>
        <Button Content="To Celsius" HorizontalAlignment="Left" Margin="364,131,0,0" VerticalAlignment="Top" Width="107" Height="50" Click="Button_Click"/>
        <Label x:Name="AnswerL" Content="" HorizontalAlignment="Left" Margin="97,214,0,0" VerticalAlignment="Top" Width="222" Height="50" FontSize="22"/>

    </Grid>
</Window>
